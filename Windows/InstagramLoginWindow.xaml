            <WebView x:Name="webviewNew" Grid.Row="0"  Navigated="webview_Navigated"  Loaded="Webview_Loaded"   ></WebView>
           
        </Grid>
        <!--<Grid x:Name="grid1">
            <Button Text="Close" Grid.Row="1" Clicked="Button_Clicked" VerticalOptions="Start"></Button>
        </Grid>-->
        
   

